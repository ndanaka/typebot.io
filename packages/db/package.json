{
  "name": "db",
  "version": "0.1.0",
  "main": "./index.tsx",
  "types": "./index.tsx",
  "devDependencies": {
    "prisma": "latest",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@prisma/client": "latest"
  },
  "scripts": {
